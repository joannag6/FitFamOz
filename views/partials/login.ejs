
<div class="container" ng-controller="NavbarCtrl">
  <div class="row">

    <div class="col-sm-4 col-sm-push-4 log-in">
      <div class="section-name">
        <i class="material-icons back-button" ng-click="prevPage()">chevron_left</i>
        <span>Already signed up?</span>
      </div>

      <div class="signup-form-contents">

        <div class="btn facebook-login" ng-click="loginUser()">
          <i class="fa fa-facebook-square facebook-icon" aria-hidden="true"></i>
          Log In with Facebook
        </div>

        <div class="btn google-login" data-onsuccess="onSignIn" ng-click="onGoogleLogin()">
          <i class="fa fa-google-plus google-icon" aria-hidden="true"></i>
          Log In with Google
        </div>
      </br>
        <div ng-if="errorMsg">
          <div class="error-text">
            {{ errorMsg }}
          </div>
          <a ng-click="nextPage()" class="btn btn-red">Sign Up</a>
        </div>

    </div>
    </div>



  </div>
</div>

<script type="text/javascript">
  (function(){
    var p = document.createElement('script');
    p.type = 'text/javascript';
    p.async = true;
    p.src = "https://apis.google.com/js/client.js?onload=onLoadFunction";
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(p,s);
  })();
</script>
